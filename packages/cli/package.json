{
  "name": "@devbookhq/cli",
  "version": "0.1.0",
  "description": "CLI for managing Devbook environments",
  "homepage": "https://usedevbook.com",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "FoundryLabs, Inc.",
    "email": "hello@usedevbook.com",
    "url": "https://usedevbook.com"
  },
  "bugs": "https://github.com/DevbookHQ/devbook-api/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/DevbookHQ/devbook-api"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsup src/cli.ts --target node16 --platform node --minify --format esm",
    "dev": "tsup src/cli.ts --target node16 --platform node --format esm --watch"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.11",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "package-lock.json"
  ],
  "bin": {
    "dbk": "dist/cli.js"
  },
  "dependencies": {
    "@devbookhq/sdk": "workspace:*",
    "boxen": "^7.0.0",
    "chalk": "^5.1.2",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "commander": "^9.4.1",
    "config": "^3.3.8",
    "figlet": "^1.5.2",
    "inquirer": "^9.1.4",
    "progress": "^2.0.3",
    "shelljs": "^0.8.5"
  },
  "engines": {
    "node": "~16"
  }
}