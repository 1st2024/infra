{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Env build task driver - via go run",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/packages/env-build-task-driver/main.go",
      "args": [
        "-env",
        "ggjp0khvwwoqeoghu19o",
        "-build",
        "08c6d485-ae68-6d98-c9c1-adafb9f488ed"
      ],
      "console": "integratedTerminal",
      "asRoot": true,
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 24262
    },
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    },
    {
      "preLaunchTask": "prepare-debug",
      "postDebugTask": "stop-debug-docker",
      "name": "Debug in Docker",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "showLog": true,
      "trace": "log",
      "logOutput": "rpc"
    },
    {
      "name": "Attach to Docker",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "showLog": true,
      "trace": "log",
      "logOutput": "rpc"
    }
  ]
}